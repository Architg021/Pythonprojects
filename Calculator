a = int(input("enter first number:"))
b = int(input("enter second number:"))
c = int(input('''Enter 1 for addition
Enter 2 for subtraction
Enter 3 for multiplication
Enter 4 for division
Enter 5 for modulus
Enter 6 for exponent
Enter 7 for floor division\n'''))
if c == 1:
  print(a + b)
elif c == 2:
  print(a - b)
elif c == 3:
  print(a * b)
elif c == 4:
  print(a / b)
elif c == 5:
  print(a % b)
elif c == 6:
  print(a ** b)
elif c==7:
  print(a // b)
else:
  print("wrong choice")
